groupSizes = [3,3,3,3,3,1,3]
work_list = groupSizes.copy()
result = []

i = 0
while groupSizes:
    current = min(groupSizes)
    result.append([])
    for j in range(len(work_list)):
        if work_list[j] == current:
            result[i].append(j)
            del groupSizes[]
        if len(result[i]) == 3:
            break
            
    i += 1
    print(result)
